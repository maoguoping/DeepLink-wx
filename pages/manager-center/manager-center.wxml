<!--pages/manager-center/manager-center.wxml-->
<view class="page">
  <scroll-view style='width:100%'>
    <manage-list id="manageList" list="{{listData}}" bind:listTap="tapItem"></manage-list>
  </scroll-view>
  <hover-menu show ="{{showHoverMenu}}">
     <image class="hover-btn" src='../../images/plus-circle.png' style='width:50px;height:50px' bind:tap="add"></image>
     <image class="hover-btn" src='../../images/info-circle.png' style='width:50px;height:50px'></image>
  </hover-menu>
  <edit-project show="{{showEditProject}}" bind:close="closeDialog"></edit-project>  
</view>